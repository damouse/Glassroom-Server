<div class="form-actions">
  <%= link_to 'Create New', '#', :class => 'btn btn-mini btn-success'  %>
  </div>

  <table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Created At</th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% media.each do |element| %>
      <tr>
        <td><%= element.name %></td>
        <td><%= element.created_at %></td>
        <td>
          <%= link_to 'Delete', '#', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<hr>


